{
    "name": "laudeco/dolibarr_stock_manager",
    "type": "project",
    "require": {
        "php": "^7.2",
        "laudeco/dolibarr-client": "^0.3.1",
        "symfony/console": "^5.0",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "^5.0",
        "symfony/yaml": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/dotenv": "^5.0",
        "ext-json": "*",
        "symfony/security-bundle": "^5.0",
        "symfony/twig-pack": "^1.0"
    },
    "require-dev": {
        "symfony/profiler-pack": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/phpunit-bridge": "^5.0",
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^0.12.8"
    },
    "authors": [
        {
            "name": "Laurent De Coninck",
            "email": "lau.deconinck@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
